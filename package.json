{
  "name": "codex-project",
  "version": "0.1.0",
  "description": "Minimal Node.js project for OpenAI code generation (Codex-style)",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:api": "node index.js",
    "generate": "node src/index.js",
