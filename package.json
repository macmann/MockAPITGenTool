    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
